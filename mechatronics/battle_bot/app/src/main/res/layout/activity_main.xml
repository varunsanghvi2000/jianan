<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.vikrant.battle_bot.MainActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@android:color/background_dark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/left_bar"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        android:max="10"
        android:padding="?android:attr/dialogPreferredPadding"
        android:progress="5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.106" />

    <SeekBar
        android:id="@+id/right_bar"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        android:max="10"
        android:padding="?android:attr/dialogPreferredPadding"
        android:progress="5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.907" />

    <TextView
        android:id="@+id/Red_nexus_health"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@color/colorPrimary"
        android:padding="?android:attr/dialogPreferredPadding"
        android:rotation="90"
        android:text="50"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.976"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.257"
        tools:text="50" />

    <TextView
        android:id="@+id/Red1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:background="@color/colorPrimary"
        android:padding="?android:attr/dialogPreferredPadding"
        android:rotation="90"
        android:text="50"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/Red_nexus_health"
        app:layout_constraintEnd_toStartOf="@+id/Red_nexus_health"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/Red2"
        app:layout_constraintTop_toTopOf="@+id/Red_nexus_health"
        tools:text="50" tools:targetApi="lollipop_mr1" />

    <TextView
        android:id="@+id/Red2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:background="@color/colorPrimary"
        android:padding="?android:attr/dialogPreferredPadding"
        android:rotation="90"
        android:text="50"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/Red1"
        app:layout_constraintStart_toEndOf="@+id/Red3"
        app:layout_constraintTop_toTopOf="@+id/Red1"
        tools:text="50" />

    <TextView
        android:id="@+id/Red3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:background="@color/colorPrimary"
        android:padding="?android:attr/dialogPreferredPadding"
        android:rotation="90"
        android:text="50"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/Red2"
        app:layout_constraintStart_toEndOf="@+id/Red4"
        app:layout_constraintTop_toTopOf="@+id/Red2"
        tools:text="50" />

    <TextView
        android:id="@+id/Red4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        android:padding="?android:attr/dialogPreferredPadding"
        android:rotation="90"
        android:text="50"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/Red3"
        app:layout_constraintTop_toTopOf="@+id/Red3"
        tools:layout_editor_absoluteX="72dp"
        tools:text="50" />

    <TextView
        android:id="@+id/own_health"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/holo_green_dark"
        android:padding="?android:attr/dialogPreferredPadding"
        android:rotation="90"
        android:text="50"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.976"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="50" />

    <TextView
        android:id="@+id/Blue_nexus_health"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/holo_blue_dark"
        android:padding="?android:attr/dialogPreferredPadding"
        android:rotation="90"
        android:text="50"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.976"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.751"
        tools:text="50" />

    <TextView
        android:id="@+id/Blue1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:background="@android:color/holo_blue_dark"
        android:padding="?android:attr/dialogPreferredPadding"
        android:rotation="90"
        android:text="50"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@+id/Blue_nexus_health"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/Blue3"
        app:layout_constraintTop_toTopOf="@+id/Blue_nexus_health"
        tools:text="50" />

    <TextView
        android:id="@+id/Blue2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:background="@android:color/holo_blue_dark"
        android:padding="?android:attr/dialogPreferredPadding"
        android:rotation="90"
        android:text="50"
        android:visibility="visible"
        app:layout_constraintStart_toEndOf="@+id/Blue4"
        app:layout_constraintTop_toTopOf="@+id/Blue3"
        tools:text="50" />

    <TextView
        android:id="@+id/Blue3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:background="@android:color/holo_blue_dark"
        android:padding="?android:attr/dialogPreferredPadding"
        android:rotation="90"
        android:text="50"
        android:visibility="visible"
        app:layout_constraintStart_toEndOf="@+id/Blue2"
        app:layout_constraintTop_toTopOf="@+id/Blue1"
        tools:text="50" />

    <TextView
        android:id="@+id/Blue4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/holo_blue_dark"
        android:padding="?android:attr/dialogPreferredPadding"
        android:rotation="90"
        android:text="50"
        android:visibility="visible"
        app:layout_constraintTop_toTopOf="@+id/Blue2"
        tools:layout_editor_absoluteX="72dp"
        tools:text="50" />

</android.support.constraint.ConstraintLayout>
